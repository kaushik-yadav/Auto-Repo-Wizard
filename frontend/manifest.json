{
  "manifest_version": 3,
  "name": "GitHub Clone Extension",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "logo/icon.png",
    "48": "logo/icon.png",
    "128": "logo/icon.png"
  }
}